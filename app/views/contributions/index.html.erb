<h1>Listing contributions</h1>

<table>
  <tr>
    <th>First item</th>
    <th>Second item</th>
    <th>User</th>
    <th>First item grade</th>
    <th>Second item grade</th>
    <th>Opinion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contributions.each do |contribution| %>
  <tr>
    <td><%= contribution.first_item_id %></td>
    <td><%= contribution.second_item_id %></td>
    <td><%= contribution.user_id %></td>
    <td><%= contribution.first_item_grade %></td>
    <td><%= contribution.second_item_grade %></td>
    <td><%= contribution.opinion %></td>
    <td><%= link_to 'Show', contribution %></td>
    <td><%= link_to 'Edit', edit_contribution_path(contribution) %></td>
    <td><%= link_to 'Destroy', contribution, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contribution', new_contribution_path %>
