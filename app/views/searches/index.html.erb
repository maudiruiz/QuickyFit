<h1>Listing searches</h1>

<table>
  <tr>
    <th>First item</th>
    <th>Second item</th>
    <th>User</th>
    <th>Is contribution</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @searches.each do |search| %>
  <tr>
    <td><%= search.first_item_id %></td>
    <td><%= search.second_item_id %></td>
    <td><%= search.user_id %></td>
    <td><%= search.is_contribution %></td>
    <td><%= link_to 'Show', search %></td>
    <td><%= link_to 'Edit', edit_search_path(search) %></td>
    <td><%= link_to 'Destroy', search, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>





